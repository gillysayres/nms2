<h1>Imports</h1>
<table class= "table table-striped table-bordered">
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
  </thead>
  <tbody>
    <% @sales.each do |import| %>
    <tr>
      <td><%= import.purchaser_name %> </td>
      <td><%= import.item_description %> </td>
      <td><%= import.item_price %> </td>
      <td><%= import.purchase_count %> </td>
      <td><%= import.merchant_address %> </td>
      <td><%= import.merchant_name %> </td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= form_tag import_sales_imports_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload"%>
<% end %>
